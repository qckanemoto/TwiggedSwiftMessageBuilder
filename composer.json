{
    "name": "ttskch/twigged-swiftmessage-builder",
    "description": "Twig templated Swift_Message builder service.",
    "keywords": ["Twig", "Swift_Message", "Swift_Mailer"],
    "homepage": "https://github.com/ttskch/TwiggedSwiftMessageBuilder",
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Takashi Kanemoto",
            "email": "kanemoto.takashi@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "swiftmailer/swiftmailer": "5.2.1",
        "twig/twig": "~1.12",
        "tijsverkoyen/css-to-inline-styles": "~1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phake/phake": "~2.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Ttskch\\TwiggedSwiftMessageBuilder\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
